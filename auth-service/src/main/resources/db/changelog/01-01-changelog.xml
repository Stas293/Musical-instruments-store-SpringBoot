<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1723294152176-1" author="stasskorobogatov">
        <createSequence sequenceName="revinfo_seq" startValue="1" incrementBy="50"/>
    </changeSet>
    <changeSet id="1723294152176-4" author="stasskorobogatov">
        <createTable tableName="revinfo">
            <column name="rev" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="revinfo_pkey"/>
            </column>
            <column name="revtstmp" type="BIGINT"/>
        </createTable>
    </changeSet>
    <changeSet id="1723294152176-5" author="stasskorobogatov">
        <createTable tableName="role">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="role_pkey"/>
            </column>
            <column name="code" type="VARCHAR(255)"/>
            <column name="name" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1723294152176-6" author="stasskorobogatov">
        <createTable tableName="user_list">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="user_list_pkey"/>
            </column>
            <column name="first_name" type="VARCHAR(255)"/>
            <column name="last_name" type="VARCHAR(255)"/>
            <column name="login" type="VARCHAR(255)"/>
            <column name="email" type="VARCHAR(255)"/>
            <column name="password" type="VARCHAR(255)"/>
            <column name="phone" type="VARCHAR(255)"/>
            <column name="enabled" type="BOOLEAN"/>
            <column name="created_at" type="timestamp"/>
            <column name="modified_at" type="timestamp"/>
            <column name="created_by" type="VARCHAR(255)"/>
            <column name="modified_by" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1723294152176-7" author="stasskorobogatov">
        <createTable tableName="user_list_aud">
            <column name="rev" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="user_list_aud_pkey"/>
            </column>
            <column name="revtype" type="SMALLINT"/>
            <column name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="user_list_aud_pkey"/>
            </column>
            <column name="email" type="VARCHAR(255)"/>
            <column name="first_name" type="VARCHAR(255)"/>
            <column name="last_name" type="VARCHAR(255)"/>
            <column name="login" type="VARCHAR(255)"/>
            <column name="password" type="VARCHAR(255)"/>
            <column name="phone" type="VARCHAR(255)"/>
            <column name="enabled" type="BOOLEAN"/>
        </createTable>
    </changeSet>
    <changeSet id="1723294152176-8" author="stasskorobogatov">
        <createTable tableName="user_role">
            <column name="role_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1723294152176-9" author="stasskorobogatov">
        <addForeignKeyConstraint baseColumnNames="user_id" baseTableName="user_role"
                                 constraintName="fk5gnwt4g6cdo4o3ht1rihhnvjf" onDelete="CASCADE"
                                 referencedColumnNames="id" referencedTableName="user_list"/>
    </changeSet>
    <changeSet id="1723294152176-10" author="stasskorobogatov">
        <addForeignKeyConstraint baseColumnNames="role_id" baseTableName="user_role"
                                 constraintName="fka68196081fvovjhkek5m97n3y" onDelete="NO ACTION"
                                 referencedColumnNames="id" referencedTableName="role"/>
    </changeSet>
    <changeSet id="1723294152176-11" author="stasskorobogatov">
        <addForeignKeyConstraint baseColumnNames="rev" baseTableName="user_list_aud"
                                 constraintName="fksatu1ttiukao5pd6a0hfsek5l" onDelete="NO ACTION"
                                 referencedColumnNames="rev" referencedTableName="revinfo"/>
    </changeSet>

    <changeSet id="1723294152176-12" author="stasskorobogatov">
        <insert tableName="role">
            <column name="name">User</column>
            <column name="code">ROLE_USER</column>
        </insert>
        <insert tableName="role">
            <column name="name">seller</column>
            <column name="code">ROLE_SELLER</column>
        </insert>
        <insert tableName="role">
            <column name="name">Admin</column>
            <column name="code">ROLE_ADMIN</column>
        </insert>
    </changeSet>

</databaseChangeLog>